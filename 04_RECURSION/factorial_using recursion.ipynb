    "def fact(n):\n",
    "    if (n==1):\n",
    "        return n\n",
    "    else:\n",
    "        return n*fact(n-1)\n",
    "x=int(input(\"enter a number \"))\n",
    "res=fact(x)\n",
    "print(res)"
